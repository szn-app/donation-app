[
  {
    "id": "api-private-TODO",
    "match": {
      "url": "<[a-zA-Z][a-zA-Z0-9+.-]*>://api.<([a-zA-Z0-9-]+.)*[a-zA-Z0-9-]+.[a-zA-Z]{2,}>/private</?><[.*]></?>[.*]",
      "methods": ["GET", "POST", "OPTIONS", "PUT", "PATCH", "HEAD", "DELETE"]
    },
    "authenticators": [
      {
        "handler": "oauth2_introspection",
        "config": {
          "scope_strategy": "none",
          "token_from": {
            "cookie": "ory_kratos_session"
          }
        }
      }
    ],
    "authorizer": {
        "handler": "deny"
    },
    "mutators": [
      {
        "handler": "header",
        "config": null
      }
    ],
    "errors": [
      {
        "handler": "json"
      }
    ]
  },

  {
    "id": "api-allow-public",
    "match": {
      "url": "<[a-zA-Z][a-zA-Z0-9+.-]*>://api.<([a-zA-Z0-9-]+.)*[a-zA-Z0-9-]+.[a-zA-Z]{2,}>/public</?><.*>",
      "methods": ["GET", "POST", "OPTIONS", "PUT", "PATCH", "HEAD", "DELETE"]
    },
    "authenticators": [
      {
        "handler": "anonymous"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "header",
        "config": null
      }
    ],
    "errors": [
      {
        "handler": "json"
      }
    ]
  }
]

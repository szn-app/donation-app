{
  "name": "web-server",
  "private": true,
  "version": "0.0.80",
  "type": "module",
  "scripts": {
    "predev": "pnpm generate-graphql",
    "prebuild": "pnpm generate-graphql",
    "dev": "vite dev",
    "dev-service-port": "vite dev --port 80 --host 0.0.0.0",
    "build": "tsc && vite build",
    "build-dev": "NODE_ENV=development tsc && NODE_ENV=development VITE_DEV_MODE=true vite build --mode development",
    "preview": "tsc && vite build && vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint ./src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "ci:check-all": "pnpm run typecheck && pnpm vitest run && pnpm vitest --ui run",
    "generate-graphql": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@heroui/react": "^2.7.8",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@radix-ui/themes": "^3.2.1",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-router": "^1.120.3",
    "@tanstack/react-table": "^8.21.3",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-shell": "^2.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "graphql": "^16.11.0",
    "graphql-request": "^7.1.2",
    "lucide-react": "^0.469.0",
    "oidc-client-ts": "^3.2.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.5.0",
    "react-oidc-context": "^3.3.0",
    "universal-cookie": "^8.0.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@eslint/js": "^9.26.0",
    "@graphql-codegen/cli": "5.0.6",
    "@graphql-codegen/client-preset": "4.8.1",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@tanstack/react-query-devtools": "^5.76.1",
    "@tanstack/react-router-devtools": "^1.120.3",
    "@tanstack/router-plugin": "^1.120.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^15.15.0",
    "graphql-codegen-typescript-validation-schema": "^0.17.1",
    "graphql-config": "^5.1.5",
    "happy-dom": "^16.8.1",
    "heroui-cli": "^1.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-dom": "^7.6.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vitest-fetch-mock": "^0.4.5"
  }
}
